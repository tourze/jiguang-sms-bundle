{
  "name": "tourze/jiguang-sms-bundle",
  "description": "极光短信",
  "type": "symfony-bundle",
  "require": {
    "php": "^8.1",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^2.17 || ^3.0",
    "symfony/framework-bundle": "^6.4",
    "tourze/easy-admin-attribute": "0.0.*",
    "tourze/doctrine-indexed-bundle": "0.0.*",
    "tourze/doctrine-track-bundle": "0.0.*",
    "tourze/doctrine-timestamp-bundle": "0.0.*",
    "tourze/doctrine-user-bundle": "0.0.*",
    "tourze/http-client-bundle": "0.0.*",
    "tourze/symfony-cron-job-bundle": "0.0.*",
    "yiisoft/json": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "JiguangSmsBundle\\": "src"
